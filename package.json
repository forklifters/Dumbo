{
  "name": "dumbo",
  "version": "1.0.0",
  "description": "The Markdown Editor",
  "main": "index.js",
  "repository": "ssh://git@github.com/sosukesuzuki/Dumbo.git",
  "author": "Sosuke Suzuki <aosukeke@gmail.com>",
  "scripts": {
    "build:webpack": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack",
    "dev:webpack": "cross-env TS_NODE_PROJECT=\"tsconfig.json\" webpack-dev-server --port=3000",
    "build": "yarn clean && tsc",
    "dev:electron": "cross-env NODE_ENV=development electron ./dist/main.js",
    "clean": "rimraf dist",
    "format": "prettier --write \"{,**/}*.{ts,tsx,json,html,md}\""
  },
  "license": "MIT",
  "devDependencies": {
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/webpack": "^4.32.1",
    "@types/webpack-dev-server": "^3.1.7",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "file-loader": "^4.1.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.5.3",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "electron": "^5.0.8"
  }
}
